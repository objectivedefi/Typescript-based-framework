{
  "name": "risk-assessment-framework",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript-based risk assessment framework with modular sections, crawler, and CLI",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
